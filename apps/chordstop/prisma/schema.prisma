// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./local.db"
}

model Chord {
  id        Int     @id @default(autoincrement())
  artist    String
  title     String
  lastedit  String?
  category  String?
  hits      Int?
  body      String
  open      Int?
}
